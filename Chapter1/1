#include <stdio.h>

int main(void) {
	int c, nb, nt, nn;
	nb, nt, nn = 0;
	while ((c = getchar()) != EOF)
		if (c == ' ' ) 
			++nb;
		if (c == '\t')
			++nt;
		if (c == '\n' )
			++nn;
	printf("%s, %d\n","Number Blanks", nb);
	printf("%s, %d\n","Number Tabs", nt);
	printf("%s, %d\n","Number Lines", nn);
}
